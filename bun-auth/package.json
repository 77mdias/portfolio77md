    "better-auth": "^1.3.34",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "drizzle-typebox": "^0.3.3",
    "elysia": "latest",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "bun-types": "latest",
    "drizzle-kit": "^0.31.6"
  },
  "module": "src/index.js"
